   "source": [
    "#Your Code Here\n",
    "\n",
    "means = df.mean(axis=1)\n",
    "std_devs = df.std(axis=1)\n",
    "conf_intervals = 1.96 * (std_devs / np.sqrt(df.shape[1]))\n",
    "print(\"means \\n\",means,\"\\n std_devs \\n\", std_devs, \"\\n conf_intervals \\n\", conf_intervals)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Choose y-axis value of interest\n",
    "y_value_of_interest = 42000\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
